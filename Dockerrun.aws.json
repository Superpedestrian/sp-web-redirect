{
  "AWSEBDockerrunVersion": 2,
  "volumes": [],
  "containerDefinitions": [
    {
      "name": "sp-web-redirect-support",
      "image": "superpedestrian/sp-web-redirect:release-0.1.2",
      "essential": true,
      "memory": 128,
      "mountPoints": [
        {
          "sourceVolume": "awseb-logs-sp-web-redirect-support",
          "containerPath": "/var/log/nginx"
        }
      ],
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ]        
    }
  ]
}
